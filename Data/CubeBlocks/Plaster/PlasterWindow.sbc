  <Definition xsi:type="MyObjectBuilder_AttachmentAnimationComponentDefinition">
    <Id Type="AttachmentAnimationComponent" Subtype="PlasterWindowShutter" />
    <Sequence Name="Open" WrapMode="Once">
      <Event Time="0.0" Name="DoorOpening" />
      <Event Time="0.4" Name="DoorMoving" />
      <Event Time="1.6" Name="DoorOpened" />

      <Animation AttachmentPoint="RoundShutterRight">
        <Key Time="0">
          <OrientationEuler x="-90" y="0" z="0" />
        </Key>
        <Key Time="0.4">
          <Interpolation Method="QuadraticEaseIn" />
          <OrientationEuler x="-90" y="0" z="0" />
        </Key>
        <Key Time="1.6">
          <OrientationEuler x="0" y="0" z="0" />
        </Key>
      </Animation>
      <Animation AttachmentPoint="RoundShutterLeft">
        <Key Time="0">
          <OrientationEuler x="90" y="0" z="0" />
        </Key>
        <Key Time="0.4">
          <Interpolation Method="QuadraticEaseIn" />
          <OrientationEuler x="90" y="0" z="0" />
        </Key>
        <Key Time="1.6">
          <OrientationEuler x="0" y="0" z="0" />
        </Key>
      </Animation>
      <Animation AttachmentPoint="RoundShutterLock">
        <Key Time="0">
          <Interpolation Method="QuarticEaseOut" />
          <Position x="-0.08" y="0" z="0" />
          <OrientationEuler x="-90" y="0" z="0" />
        </Key>
        <Key Time="0.4">
          <Interpolation Method="QuadraticEaseIn" />
          <OrientationEuler x="-90" y="0" z="0" />
        </Key>
        <Key Time="1.6">
          <OrientationEuler x="0" y="0" z="0" />
        </Key>
      </Animation>
    </Sequence>
    <Sequence Name="Close" WrapMode="Once">
      <Event Time="0.0" Name="DoorClosing" />
      <Event Time="0.1" Name="DoorMoving" />
      <Event Time="1.6" Name="DoorClosed" />

      <Animation AttachmentPoint="RoundShutterRight">
        <Key Time="0">
          <Interpolation Method="QuadraticEaseIn" />
          <OrientationEuler x="0" y="0" z="0" />
        </Key>
        <Key Time="1.2">
          <OrientationEuler x="-90" y="0" z="0" />
        </Key>
      </Animation>
      <Animation AttachmentPoint="RoundShutterLeft">
        <Key Time="0">
          <Interpolation Method="QuadraticEaseIn" />
          <OrientationEuler x="0" y="0" z="0" />
        </Key>
        <Key Time="1.2">
          <OrientationEuler x="90" y="0" z="0" />
        </Key>
      </Animation>
      <Animation AttachmentPoint="RoundShutterLock">
        <Key Time="0">
          <Interpolation Method="QuadraticEaseIn" />
          <OrientationEuler x="0" y="0" z="0" />
        </Key>
        <Key Time="1.2">
          <Interpolation Method="QuarticEaseIn" />
          <OrientationEuler x="-90" y="0" z="0" />
        </Key>
        <Key Time="1.6">
          <Position x="-0.08" y="0" z="0" />
          <OrientationEuler x="-90" y="0" z="0" />
        </Key>
      </Animation>
    </Sequence>
  </Definition>

