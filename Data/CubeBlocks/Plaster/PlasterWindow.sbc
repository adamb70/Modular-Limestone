  <Definition xsi:type="MyObjectBuilder_ModelAttachmentComponentDefinition">
    <Id Type="ModelAttachmentComponent" Subtype="PlasterWindowShutter" />
    <Attachment Name="RoundShutterRight">
      <Offset x="0.28617" y="0.24878" z="-0.782774" />
      <Rotation x="0" y="0" z="0" />
      <DefaultEntity Type="EntityBase" Subtype="ShuttersWoodRoundRight" />
    </Attachment>
    <Attachment Name="RoundShutterLeft">
      <Offset x="-0.2881" y="0.24878" z="-0.782774" />
      <Rotation x="0" y="0" z="0" />
      <DefaultEntity Type="EntityBase" Subtype="ShuttersWoodRoundLeft" />
